            <diffuse r="1" g="1" b="1" a="1"/>        
            <specular r="1" g="1" b="1" a="1"/>  
        </omni>

        <spot id="lampRight" enabled="1" angle="-160" exponent="1.0">
            <target x="9" y="0.67" z="4" />
