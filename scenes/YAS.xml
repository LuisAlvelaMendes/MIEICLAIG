<yas>

    <scene root="root" axis_length="5.0" />

    <views default="default" >

        <perspective id="default" near="0.1" far="550" angle="62">
            <from x="16" y="6" z="18" />
            <to x="0" y="0" z="0" />
        </perspective>
        
        <perspective id="view1" near="0.1" far="550" angle="20">
            <from x="40" y="20" z="30" />
            <to x="0" y="1" z="0" />
        </perspective>

        <perspective id="view2" near="0.1" far="550" angle="30">
            <from x="30" y="5" z="10" />
            <to x="-10" y="1" z="5" />
        </perspective>
        

        <ortho id="ortho" near="0.1" far="500"
               left="-5" right="5" top="5" bottom="-5">
            <from x="40" y="20" z="30" />
            <to x="0" y="1" z="0" />
        </ortho>

    </views>

